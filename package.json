{
    "name": "roadside_station_maps",
    "version": "0.1.0",
    "files": [
        "data",
        "html"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tk0miya/roadside_station_maps"
    },
    "main": "html/js/bundle.js",
    "scripts": {
        "start": "node -e \"require('./esbuild.config.js').watch()\"",
        "build": "node -e \"require('./esbuild.config.js').build()\"",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "format": "biome format --write .",
        "typecheck": "tsc --noEmit",
        "serve": "node -e \"require('./esbuild.config.js').serve().then(s => console.log('Server at http://localhost:' + s.port))\"",
        "dev": "npm run serve",
        "generate:geojson": "npx tsx src/scripts/generate_geojson.ts",
        "generate:stations": "npx tsx src/scripts/generate_stationlist.ts",
        "generate:all": "npm run generate:stations && npm run generate:geojson",
        "test": "vitest"
    },
    "dependencies": {
        "cheerio": "^1.1.0",
        "clipboard": "^2.0.11",
        "create-react-class": "^15.7.0",
        "jaconv": "^1.0.4",
        "jquery": "^3.7.1",
        "prop-types": "^15.8.1",
        "query-string": "^9.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.0.0",
        "@types/cheerio": "^0.22.35",
        "@types/clipboard": "^2.0.1",
        "@types/google.maps": "^3.58.1",
        "@types/jquery": "^3.5.31",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "esbuild": "^0.25.5",
        "typescript": "^5.7.2",
        "vitest": "^3.2.4"
    }
}
