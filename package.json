{
    "name": "roadside_station_maps",
    "version": "0.1.0",
    "files": [
        "data",
        "html"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tk0miya/roadside_station_maps"
    },
    "main": "html/js/app.js",
    "babel": {
        "presets": [
            ["@babel/preset-env", {"targets": {"browsers": ["last 2 versions"]}}],
            ["@babel/preset-react", {"runtime": "automatic"}]
        ]
    },
    "scripts": {
        "start": "gulp watch",
        "build": "gulp build",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "format": "biome format --write .",
        "typecheck": "tsc --noEmit",
        "serve": "gulp serve",
        "dev": "gulp",
        "generate:geojson": "npx tsx src/scripts/generate_geojson.ts",
        "generate:stations": "npx tsx src/scripts/generate_stationlist.ts",
        "generate:all": "npm run generate:stations && npm run generate:geojson",
        "test": "vitest"
    },
    "dependencies": {
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "cheerio": "^1.1.0",
        "clipboard": "^2.0.11",
        "gulp": "^5.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-webserver": "^0.9.1",
        "jaconv": "^1.0.4",
        "jquery": "^3.7.1",
        "query-string": "^7.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "watchify": "^4.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-react": "^7.27.1",
        "@biomejs/biome": "^2.0.0",
        "@types/cheerio": "^0.22.35",
        "@types/jquery": "^3.5.31",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "tsify": "^5.0.4",
        "typescript": "^5.7.2",
        "vitest": "^3.2.4"
    }
}
